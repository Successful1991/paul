	.minicard
		.minicard__content
			.minicard__top
				.minicard__title
					span.minicard__title-big Shopping Basket
				.minicard__close.js-minicard-close
					img(src="./assets/images/close-popup.svg")
			.minicard__center
				.minicard__list.js-basket__list
			.subtotal
				.subtotal__name Subtotal:
				.subtotal__sum
					span.js-subtotal__sum 0
					| UAH
			form.minicard__form(id='payProduct')
				label.minicard__username.minicard__label
					span Name and surname:
					input(type='text' name="name" required).minicard__input.js-form__input
				label.minicard__phone.minicard__label
					span Phone number:
					input(type='text' name="phone" required).minicard__input.js-form__input.inputTelMask
				label.minicard__mail.minicard__label
					span E-mail:
					input(type='mail' name="mail" required).minicard__input.js-form__input
				label.minicard__date.minicard__label
					span Collection Date:
					input(type='text' name="date" required).minicard__input.js-minicard__date.js-form__input
				label.minicard__date.minicard__label
					span Collection Time:
					select(name="time").minicard__input.js-minicard__time.js-form__input
						option(value=11) 11:00 - 12:00
						option(value=12) 12:00 - 13:00
						option(value=13) 13:00 - 14:00
				fieldset.minicard__delivery.minicard__label
					span Delivery:
					input(type='radio' name='delivery' id="delivery-1" value="sm1" checked).minicard__input.js-minicard__delivery.js-form__input
					label(for='delivery-1')
						span Pickup Gorodetskogo, 6
					input(type='radio' name='delivery' id="delivery-2" value="sm2").minicard__input.js-minicard__delivery.js-form__input
					label(for='delivery-2')
						span Pickup Yaroslavov Val, 26
					input(type='radio' name='delivery' id="delivery-3" value="devRight" data-value=80 ).minicard__input.js-minicard__delivery.js-form__input
					label(for='delivery-3')
						span Courier Right Bank (delivery 80 UAH)
					input(type='radio' name='delivery' id="delivery-4" value="devLeft" data-value=150 ).minicard__input.js-minicard__delivery.js-form__input
					label(for='delivery-4')
						span Courier Left Bank (delivery 150 UAH)
				fieldset.minicard__payment.minicard__label
					span Payment:
					input(type='radio' name='payment' value='cash' id="pay-1" checked).minicard__input.js-form__input
					label(for='pay-1')
						span Cash
					input(type='radio' name='payment' id="pay-2" value='card').minicard__input.js-form__input
					label(for='pay-2')
						span Visa / Mastercard payment (liqpay)
				input(type="text" name="products" class="is-hidden js-products__input")


			.minicard__price
				.minicard__price--subtotal Subtotal:
					span.js-minicard__price--subtotal(data-value=0) 0
					| UAH
				.minicard__price--delivery Delivery Cost:
					span.js-minicard__price--delivery(data-value=0) 0
					| UAH
			.minicard__bottom
				.total
					.total__name Total:
					.total__sum
						span.js-total__sum(data-value=0) 0
						| â‚´
				.minicard__links
					button(form='payProduct').link-dark.js-minicard__pay PROCEED TO PAYMENT






